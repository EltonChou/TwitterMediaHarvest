    "@typescript-eslint/parser": "^5.28.0",
    "babel-jest": "^28.1.1",
    "babel-loader": "^8.2.3",
    "babel-register": "^6.26.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.0",
