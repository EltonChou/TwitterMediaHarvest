    "**/@typescript-eslint/eslint-plugin": "^4.15.1",
    "**/@typescript-eslint/parser": "^4.15.1"
  },
  "scripts": {
    "test": "jest --env=node --coverage",
    "build": "webpack --mode=production --progress --color",
    "build-dev": "webpack --mode=development --progress --color",
    "build-win": "webpack --mode=production --progress --color && powershell Compress-Archive -Force build/ media_harvest.zip",
